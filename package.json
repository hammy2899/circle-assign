{
  "name": "circle-assign",
  "version": "1.0.10",
  "description": "Simple deep object assign function",
  "main": "src/index.js",
  "module": "dist/circle-assign.js",
  "scripts": {
    "test": "./node_modules/.bin/jest --coverage --verbose",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "build:clean": "rm -rf dist/",
    "build:babel": "mkdir dist && npx babel src/index.js --out-file dist/circle-assign.js",
    "build:uglify": "npx uglifyjs dist/circle-assign.js -o dist/circle-assign.min.js",
    "build": "npm-run-all build:clean build:babel build:uglify"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/hammy2899/circle-assign.git"
  },
  "keywords": [
    "object",
    "assign",
    "extend",
    "deep",
    "recursive",
    "merge"
  ],
  "author": "hammy2899",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hammy2899/circle-assign/issues"
  },
  "homepage": "https://github.com/hammy2899/circle-assign#readme",
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "coveralls": "^3.0.2",
    "jest": "^23.6.0",
    "npm-run-all": "^4.1.3",
    "uglify-js": "^3.4.9"
  }
}
